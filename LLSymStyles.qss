QTabWidget::pane {
            background-color: lightgrey;
            border: 2px solid white;
}

#TabWidgetFrame {
            background-color: #a1a1a1;
            border: 1px solid lightgrey;
}

#tw_LLX_Symbols {
            color:#000000;
            background: lightyellow; /*Change the background color*/
            alternate-background-color: lightgrey;
            Border-style:plain;/*Change the border style*/
            border-radius: 1px;
            padding-left:1px;
            padding-top:1px;
}

QTextBrowser #tb_StartUpInfo {
    color:#000000;
    Background:lightyellow; /*Change the background color*/
    Border-style:plain;/*Change the border style*/
    border-radius: 1px;
    padding-left:4px;
    padding-top:4px;
    }
